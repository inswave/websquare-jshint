		<script type="javascript"><![CDATA[
			// 페이지 공통객체
			var PagePram = {}, PageFunc = {};
			// 나의정보보기 메인 객체
			PageFunc.myPage = (function(myPage) {
				// 고객 등급 객체
				var userGrade = {
					'001' : 'prestige',
					'002' : 'royal',
					'003' : 'ace',
					'005' : 'general'
				};
				// 1. 로그인 정보
				function getLoginInfo() {
					dwsSession.getSession(function(result) {
						if (result.isLogin){
							txt_userInfo.setValue('<span><strong>' + result.userName  + '</strong>( ' + userGrade[result.userGrade] + ')</span>님 환영합니다.');
						}
					});
				}
				// 2. 총자산, 평가손익 정보 호출 : HTS 6101, W67OI01
				function selectMyAssetSummary() {
					DWSAjax.post('/mkm/main/selectMyAssetSummary.do', {}, function(response) {
						// 나의자산 차트 그리기
						drawMyAssetChart();
						// 자산현황 셋팅
						setMyAssetStatus();
						// 잔고현황 셋팅
						setMyAssetRemains(response.dc_w67oi01Grid1);
					});
				}
				// 3. 연락처정보 조회
				function selectCustInfo() {
					DWSAjax.post('/mkm/main/selectCustInfo.do', null, function(response) {
						if(response && response.dc_a11oia5Data) {
							txt_hp.setValue(response.dc_a11oia5Data.o_HpNo.telFormat());
							txt_email.setValue(response.dc_a11oia5Data.o_Email);
							txt_addr.setValue(response.dc_a11oia5Data.o_Addr);
							txt_ivstTrndClas.setValue(response.dc_a11oia5Data.o_IvstTrndClas);
							
							txt_mbhp.setValue(response.dc_a11oia5Data.o_HpNo.telFormat());
						}
					});
				}
				// 4. 거래내역조회
				function selectRecentTrade() {
					var param = {};
					param.startDt = dwsDate.addMonth(-1);
					param.endDt = dwsDate.getCurrentDate();
					DWSAjax.post('/mkm/main/selectRecentTrade.do', param, function(response) {
						var i;
						var trList = [];
						if (response && response.dc_mkm1001t04_grd) {
							var rCount = response.dc_mkm1001t04_grd.length;
							// 최근 5개 내역만 보여준다.
							for (i=0; i < rCount; i++) {
								if (rCount - 5 <= i) {
									trList.push(response.dc_mkm1001t04_grd[i]);
								}
							}
						}
						dc_tradeList.setJSON(trList);
					});
				}
				// 5. 서비스신청현황 조회
				function selectBasicAcntInfo() {
					DWSAjax.post('/mkm/main/selectBasicAcntInfo.do', null, function(response) {
						if(response && response.dc_A11OI01_Tab2_OUT_REC) {
							var data = response.dc_A11OI01_Tab2_OUT_REC;
							if(data.length == 0) {
								return;
							}
							// 사이버id등록 : 0-2, 
							txt_signText1.setValue(data[0].o_RegDate2);
							//OTP발급 : 2-1, 
							txt_signText2.setValue(data[2].o_RegDate1);
							// 주문계좌등록 : 3-1
							txt_signText3.setValue(data[3].o_RegDate1);
						}
					});
				}
				// 6. 오늘의 계좌점검
				function selectTodayAccountCheckList() {
					var showCount = 1;
					DWSAjax.post('/mkm/main/selectTodayAccountCheckList.do', null, function(response) {
						var pattern = /(dc\_grid)([0-9]*)/; 
						$.each(response, function(key, value) {
							if (showCount === 3) {
								return false;
							}
							if (pattern.test(key)) {
								var i;
								var list = response[key];
								for (i=0; i < list.length; i++) {
									if (showCount === 3) {
										return false;
									}
									$('#txt_checkAcnt' + showCount).text(list[i].o_Msg);
									showCount++;
								}
							}
						});
					});
				}
				// 자산현황 셋팅
				function setMyAssetStatus() {
					// 평가손익 +- 에따라 화살표 업,다운처리
					if (dc_w67oi01Data.get('o_EstmPlTot').toNumber() > 0) {
						txt_o_EstmPlTot.addClass('low');
					} else {
						txt_o_EstmPlTot.removeClass('low');
					}
					// 자산현황 테이블
					var assetList = {
						'예수금' : {amt : dc_w67oi01Data.get('o_DepCashAmt'), rate : dc_w67oi01Data.get('o_IvstWeight1'), pi : null}, 
						'외화예수금' : {amt : dc_w67oi01Data.get('o_FcurEstmAmt'),  rate : dc_w67oi01Data.get('o_IvstWeight2'), pi : null},
						'주식' : {amt : dc_w67oi01Data.get('o_StkEstmAmt'),  rate : dc_w67oi01Data.get('o_StkWeight'), pi : dc_w67oi01Data.get('o_StkEstmPl')},
						'권리평가' : {amt : dc_w67oi01Data.get('o_RghtEstmAmt'),  rate : dc_w67oi01Data.get('o_IvstWeight3'), pi : dc_w67oi01Data.get('o_IvstPl3')},
						'해외주식' : {amt : dc_w67oi01Data.get('o_EstmWcurCamt'), rate : dc_w67oi01Data.get('o_IvstWeight4'), pi : dc_w67oi01Data.get('o_IvstPl4')},
						'채권' : {amt : dc_w67oi01Data.get('o_BondEstmAmt'), rate : dc_w67oi01Data.get('o_IvstWeight5'), pi : dc_w67oi01Data.get('o_IvstPl5')},
						'ELS/DLS' : {amt : dc_w67oi01Data.get('o_BondEstmAmt2'), rate : dc_w67oi01Data.get('o_IvstWeight6'), pi : dc_w67oi01Data.get('o_IvstPl6')},
						'펀드' : {amt : dc_w67oi01Data.get('o_BncrEstmAmt'), rate : dc_w67oi01Data.get('o_IvstWeight7'), pi : dc_w67oi01Data.get('o_BncrIvstPl')},
						'선물/옵션' : {amt : dc_w67oi01Data.get('o_OptEstmAmt'), rate : dc_w67oi01Data.get('o_IvstWeight8'), pi : dc_w67oi01Data.get('o_FuopEstmPl')},
						'해외파생' : {amt : null, rate : null, pi : dc_w67oi01Data.get('o_FdEstmPl')},
						'신탁' : {amt : dc_w67oi01Data.get('o_TrstTot'), rate : dc_w67oi01Data.get('o_IvstWeight9'), pi : null}, 
						'퇴직연금' : {amt : dc_w67oi01Data.get('o_RetrAnnEstmAmt'), rate : dc_w67oi01Data.get('o_RetrWeigth'), pi : null}, 
						'RP' : {amt : dc_w67oi01Data.get('o_RpEstmAmt'), rate : dc_w67oi01Data.get('o_IvstWeight10'), pi : dc_w67oi01Data.get('o_IvstPl9')},
						'기타자산' : {amt : dc_w67oi01Data.get('o_EtcEstmAmt'), rate : dc_w67oi01Data.get('o_IvstWeight'), pi : dc_w67oi01Data.get('o_IvstPl')},
						'합계' : {amt : dc_w67oi01Data.get('o_TotTot'), rate : null, pi : dc_w67oi01Data.get('o_EstmPlTot')}
					};
					// 자산현황 테이블 키셋
					var keySet = [];
					$.each(assetList, function(k, v) {
						keySet.push(k);
					});
					var i;
					for (i=0; i < keySet.length; i++) {
						// 총합계
						if (i === keySet.length -1) {
							// 총평가금액, 평가손익
							txt_totEsti.setValue(dwsCommon.addComma(assetList[keySet[i]].amt));
							var value = assetList[keySet[i]].pi.toNumber();
							txt_totPi.addClass(value > 0 ? 'high' : (value < 0 ? 'low' : ''));
							txt_totPi.setValue(value > 0 ? '<span class="fl high">▲</span>' + dwsCommon.addComma(value) : (value < 0 ? '<span class="fl low">▼</span>' + dwsCommon.addComma(value) : value));
							return;
						}
						var imgAlt = ['노랑', '주황', '연두', '파랑', '남색', '분홍', '보라', '하늘색', '다홍', '녹색', '그레이블루', '브라운', '꽃분홍', '그레이'];
						// 자산평가금액이 0보다 클때 셋팅
						if (assetList[keySet[i]].amt !== null && assetList[keySet[i]].amt.toNumber()  > 0) {
							var subKey = assetList[keySet[i]];
							var index = assetSummaryGenerator.insertChild();
							var nameObj = assetSummaryGenerator.getChild(index , 'txt_assetName');
							nameObj.setValue(keySet[i]);
							var imgObj = assetSummaryGenerator.getChild(index, 'img_asset');
							var imageIndex = String(i+1).length === 1 ? ('0' + (i+1)) : i;
							imgObj.setSrc('/dws/images/contents/icon_color_circle' + imageIndex + '.png');
							imgObj.setAlt(imgAlt[i]);
							var amt = assetList[keySet[i]].amt === null ? '-' : dwsCommon.addComma(assetList[keySet[i]].amt);
							var rate = assetList[keySet[i]].rate === null ? '-' : dwsCommon.addComma(assetList[keySet[i]].rate) + '%';
							var pi = assetList[keySet[i]].pi === null ? '-' : assetList[keySet[i]].pi;
							assetSummaryGenerator.getChild(index , 'txt_amt').setValue(amt);
							assetSummaryGenerator.getChild(index , 'txt_rate').setValue(rate);
							var piObj = assetSummaryGenerator.getChild(index , 'txt_pi');
							if (pi === '-') {
								piObj.setValue(pi);
							} else {
								if (pi !== null && pi.toNumber() === 0) {
									continue;
								}
								var value = pi.toNumber();
								piObj.addClass(value > 0 ? 'high' : (value < 0 ? 'low' : ''));
								piObj.setValue(value > 0 ? '<span class="fl high">▲</span>' + dwsCommon.addComma(value) : (value < 0 ? '<span class="fl low">▼</span>' + dwsCommon.addComma(value).replace(/\-/, '') : '<span class="fl">' + dwsCommon.addComma(value)));
							}
						}
					}
				}
				// 차트 그리기
				function drawMyAssetChart() {
					var chartData = {
						'chart' : {},
						'data' : [
							// 예수금
							{'label' : dc_w67oi01Data.get('o_DepCashAmt').toNumber() > 0 ? '예수금, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_IvstWeight1')) + '%' : '', 'value' : dc_w67oi01Data.get('o_IvstWeight1').toNumber()},
							// 외화예수금
							{'label' : dc_w67oi01Data.get('o_FcurEstmAmt').toNumber() > 0 ? '외화예수금, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_IvstWeight2')) + '%' : '', 'value' : dc_w67oi01Data.get('o_IvstWeight2').toNumber()},
							// 주식
							{'label' : dc_w67oi01Data.get('o_StkEstmPl').toNumber() > 0 ? '주식, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_StkWeight')) + '%' : '', 'value' : dc_w67oi01Data.get('o_StkWeight').toNumber()},
							// 권리평가
							{'label' : dc_w67oi01Data.get('o_RghtEstmAmt').toNumber() > 0 ? '권리평가, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_IvstWeight3')) + '%' : '', 'value' : dc_w67oi01Data.get('o_IvstWeight3').toNumber()},
							// 해외주식
							{'label' : dc_w67oi01Data.get('o_EstmWcurCamt').toNumber() > 0 ? '해외주식, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_IvstWeight4')) + '%' : '', 'value' : dc_w67oi01Data.get('o_IvstWeight4').toNumber()},
							// 채권
							{'label' : dc_w67oi01Data.get('o_BondEstmAmt').toNumber() > 0 ? '채권, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_IvstWeight5')) + '%' : '', 'value' : dc_w67oi01Data.get('o_IvstWeight5').toNumber()},
							// ELS/DLS
							{'label' : dc_w67oi01Data.get('o_BondEstmAmt2').toNumber() > 0 ? 'ELS/DLS, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_IvstWeight6')) + '%' : '', 'value' : dc_w67oi01Data.get('o_IvstWeight6').toNumber()},
							// 펀드
							{'label' : dc_w67oi01Data.get('o_BncrEstmAmt').toNumber() > 0 ? '펀드, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_IvstWeight7')) + '%' : '', 'value' : dc_w67oi01Data.get('o_IvstWeight7').toNumber()},
							// 선물/옵션
							{'label' : dc_w67oi01Data.get('o_OptEstmAmt').toNumber() > 0 ? '선물/옵션, ' + dwsCommon.addComma(dc_w67oi01Data.get('o_IvstWeight8')) + '%' : '', 'value' : dc_w67oi01Data.get('o_IvstWeight8').toNumber()},
							// 해외파생 : 안보여줌
							/*{'label' : '', 'value' : dc_w67oi01Data.get('').toNumber()}, */
							// 신탁
