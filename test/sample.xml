				// 잔고현황
				function setMyAssetRemains(data) {
					// data가 없으면 return
					if (typeof data =='undefined' || !data.length > 0) return;
					// 버블정렬
					function bubbleSort() {
						var swapped = true;
						while(swapped) {
							var i;
							swapped = false;
							for (i=0; i < dataList.length - 1; i++) {
								if (dataList[i].o_Subtt.toNumber() < dataList[i + 1].o_Subtt.toNumber()) {
									var temp = dataList[i];
									dataList[i] = dataList[i+1];
									dataList[i+1] = temp;
									swapped = true;
								}
							}
						}
					}
					var i;
					var dataList = [];	// 정렬된 데이터목록
					// 내림차순 정렬하여 잔고 상위 4개항목만 셋팅한다.
					for (i=0; i < data.length; i++) {
						dataList.push(data[i]);
						bubbleSort();
					}
					// 잔고 상위 4개를 그린다.
					for (i=0; i < 4; i++) {
						// 항목 그룹 추가
						var index = assetRemainsGenerator.insert();
						if (dataList.length > i) {
							var acntObj = assetRemainsGenerator.getChild(index, 'txt_acntNo');
							acntObj.setValue(dwsCommon.formatAccount(dataList[i].o_MgmtAcntNo));
							acntObj.unbind('click');
							acntObj.bind('click', function(acntNo) {
								return function() {
									var options = {
										param : {
											accountNo : WebSquare.text.BASE64Encoder(acntNo)
										}
									};
									dwsCommon.movePage('/dws/view/mkm/mkm1/mkm1002m01.do', options);
								};
							}(dataList[i].o_MgmtAcntNo));
							assetRemainsGenerator.getChild(index, 'txt_jobSect').setValue(dataList[i].o_JobSect);
							assetRemainsGenerator.getChild(index, 'txt_amt').setValue(dwsCommon.addComma(dataList[i].o_Subtt));
							if(i==2){
								assetRemainsGenerator.getChild(index, 'group178').addClass('mb_ml0');
							}
						} else {
							assetRemainsGenerator.getChild(index, 'grp_remainData').setStyle('display', 'none');
							assetRemainsGenerator.getChild(index, 'grp_noRemainData').setStyle('display', 'block');
						}
					}
					
				}
				
				var init = (function() {
					// 그리드 초기화
					dwsCommon.gridModule.responsiveGrid('grd_recentTradeList');
